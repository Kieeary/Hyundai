  <script type="text/javascript">
    $(document).ready(function () {
      // 기본값
      //console.log("rollback jsp");
      // QuickMenu
      $('#quickmenu1, .quick-content-close__btn').on('click', function () {
        if ($('.quick').hasClass('open')) {
          $('.quick').removeClass('open');
        } else {
          $('.quick').addClass('open');
        }
      });

      if ($('.page-wrapper').length) {
        fn_swichbutton_scroll();

        $(window)
          .on('scroll', function () {
            fn_swichbutton_scroll();
          })
          .on('resize', function () {
            fn_swichbutton_scroll();
          });
      }



    });


    $("#button-quick-mall").hide();
    $('#button-quick-community').show();
    $('.quick').css({ right: '0', left: 'inherit' });
    anime({
      targets: '.page-wrapper',
      translateX: '0',
      duration: 800,
      easing: 'cubicBezier(0.17, 0.04, 0.03, 0.94)',
    });
    $('#pageMall').css({ left: '0', position: 'relative' });
    $('#pageCommunity').css({ left: '100vw', position: 'absolute' });
    $('html, body').animate({ scrollTop: 0 }, 10);


    $(window).on('scroll', function () {
      if ($('#tabDetail').length !== 0) {
        if ($('#tabDetail').offset().top <= $(window).scrollTop()) {
          $('#tabDetail .tab-detail').css({
            position: 'fixed',
          });
        } else {
          $('#tabDetail .tab-detail').css({
            position: 'absolute',
          });
        }
      }
    });

    function checkVisible(elm, eval) {
      eval = eval || 'object visible';
      var viewportHeight = $(window).height(), // Viewport Height
        scrolltop = $(window).scrollTop(), // Scroll Top
        y = $(elm).offset().top,
        elementHeight = $(elm).height();

      if (eval == 'object visible') return y < viewportHeight + scrolltop && y > scrolltop - elementHeight;
      if (eval == 'above') return y < viewportHeight + scrolltop;
    }

    // 200821, fullmenu swiper
    var swiper = new Swiper('.fullmenu-swiper', {
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
      // mousewheel: true,
      keyboard: true,
      observer: true,
      observeParents: true,
    });

    // S : 리바트몰-커뮤니티 페이지 전환 : 200903
    var swiperQuickRecent;
    function swiperLoadQuickRecent() {
      // 최근 본 상품 스와이프
      var swiperList = $('.quick-item-recent-list .swiper-slide'),
        swiperControl = $('.quick-item-recent-list .swiper-controls-wrap');

      if (swiperList.length > 4) {
        swiperQuickRecent = new Swiper('.quick-item-recent-list .swiper-container', {
          loop: false,
          direction: 'vertical',
          navigation: {
            nextEl: '.quick-item-recent-list .swiper-button-next',
            prevEl: '.quick-item-recent-list .swiper-button-prev',
          },
          paginationClickable: true,
          spaceBetween: 10,
          slidesPerView: 4,
          slidesPerGroup: 4,
        });
      } else {
        swiperList.show();
        swiperControl.hide();
      }
    }
    // 퀵 메뉴의 최근본상품~하단까지 높이 체크
    function checkQuickRecentHeight() {
      // minHeight : 퀵 메뉴의 최근본상품~하단까지 높이
      var minHeight = 270;
      var quickContentHeight = $('.quick-content').outerHeight();
      var topHeight = minHeight - quickContentHeight;
      if (topHeight < 0) {
        topHeight = topHeight + 'px';
        $('.quick-item-recent-list').css({ top: topHeight });
      } else {
        $('.quick-item-recent-list').css({ top: 0 });
      }
    }

    $(document)
      //	quick 이벤트 수정 : 201116
      // .on('mouseenter', '.button-switch-quick', function () {
      .on('mouseenter', '.quick-inner', function () {
        $(this).closest('.quick').addClass('__quick-open');
      })
      .on('mouseleave', '.quick', function () {
        $(this).closest('.quick').removeClass('__quick-open');
      })
      .on('click', '#button-quick-community', function () {
        $(location).attr("href", "https://www.hyundailivart.co.kr/community");
      })
      .on('click', '#button-quick-mall', function () {
        $(location).attr("href", "https://www.hyundailivart.co.kr/index");
      })
      .on('click', '.quick-menu-item', function (e) {
        if ($(this).attr('id') == 'button-quick-menu-recent') {
          // Quick 최근 본 상품 클릭시
          e.preventDefault();

          if ($(this).hasClass('is-active')) {
            $(this).removeClass('is-active');
            $('.quick-item-recent-list').hide();
            if (swiperQuickRecent != undefined) swiperQuickRecent.destroy();
          } else {
            $(this).addClass('is-active');
            $('.quick-item-recent-list').show();
            // 최근 본 상품 스와이프
            swiperLoadQuickRecent();

            // 퀵 메뉴의 최근본상품~하단까지 높이 체크
            checkQuickRecentHeight();
          }
        } else {
          $('#button-quick-menu-recent').removeClass('is-active');
          $('.quick-item-recent-list').hide();
          if (swiperQuickRecent != undefined) swiperQuickRecent.destroy();
        }
      })
      .on('click', '.quick-item-recent-list .btn-del', function () {
        // Quick 최근 본 상품 목록 삭제
        var tg = $(this).closest('.swiper-slide').index();
        $('.quick-item-recent-list .swiper-slide').eq(tg).remove();
        var tgLen = $('.quick-item-recent-list .swiper-slide').length;
        if (tgLen < 1) {
          $('.quick-content').hide();
          $('#button-quick-menu-recent').removeClass('is-active').addClass('is-disabled');
        }
        $('#button-quick-menu-recent .quick-menu__num').text(tgLen);
        // swiper.updateSlides();
        swiperLoadQuickRecent();

        // 퀵 메뉴의 최근본상품~하단까지 높이 체크
        checkQuickRecentHeight();
        data = "sn=" + $(this).data("val");
        deleteGoodsHistory(data);
      });

    var scrollPosition;
    function fn_scroll_updown(scr) {
      var retStr;
      if (scr > scrollPosition) {
        // scrollDown Check
        retStr = 'down';
      } else {
        // scrollUp	Check
        retStr = 'up';
      }
      scrollPosition = scr;
      return retStr;
    }

    function fn_swichbutton_scroll() {
      var winHeight = $(window).innerHeight();
      var quickHeight = $('.quick-inner').height();
      var scroll_top = $(window).scrollTop();
      var topPx = winHeight - quickHeight;
      topPx = topPx + 'px';
      if (winHeight > quickHeight) {
        $('.quick-inner').css({ top: 0 });
      } else {
        if (fn_scroll_updown(scroll_top) == 'down') {
          $('.quick-inner').css({ 'margin-top': topPx });
        } else {
          $('.quick-inner').css({ 'margin-top': 0 });
        }
      }
    }



    function deleteGoodsHistory(data) {

      $.ajax({
        url: "/goods/deleteHistory",
        dataType: "json",
        data: data,
        success: function (result) {
          //alert(result.resultCode);
          /*    if(result.resultCode === "00"){
               showHistoryList();
             }else{
                 alert(result.resultMsg);
             } */
        }
      });
    }

    function heart(obj, blbsn) {
      /*좋아요 카운트 증가,해제*/

      var $that = $(this);
      var $icoHeart = $(obj).find('.product-item-heart');
      var num = parseInt(removeComma($(obj).find('.product-item-heart__num').text()));
      $.ajax({
        url: "/community/mylove",
        data: { blbSn: blbsn },
        dataType: "json",
        success: function (result) {
          if (result.resultData === "0000") {
            if (!$icoHeart.hasClass('is-active')) {
              $icoHeart.addClass('is-active');
              num = num + 1;
            } else {
              $icoHeart.removeClass('is-active');
              num = num <= 0 ? 0 : num - 1;
            }
            var txtNum = num.toLocaleString();
            $(obj).find('.product-item-heart__num').text(txtNum);

          } else {
            $.eshopfront.cmm.alert({
              content: "좋아요는 한번만 가능합니다.",
              btn: {
                close: false,
                cancel: false
              }
            });
          }
        }
      });
      return;
    }

    function removeComma(str) {
      n = parseInt(str.replace(/,/g, ''));
      return n;
    }

    function popupCpnDownload(cpnSn, popSn) {
      //쿠폰다운로드
      if ('N' === 'N') {
        $.eshopfront.cmm.doLogin();
        return false;
      } else {
        var data = { "cpnSns": cpnSn }
        $.eshopfront.cmm.issueCoupon(data, function (result) {
          if (result.resultCode == "00") {
            var cookieNm = 'beforeUrlPopUp_' + popSn;
            $.eshopfront.cmm.removeCookie(cookieNm, ".hyundailivart.co.kr");
            $.eshopfront.cmm.setCookie(cookieNm, "N", 1, ".hyundailivart.co.kr");
            $.eshopfront.cmm.alert({
              content: "<p>쿠폰을 다운로드 받으셨습니다.</p>",
              btn: {
                close: false,
                cancel: false,
              },
              callback: {
                close: function () {
                  $(".button-modal-close", ".modal[id*='popup_']").click();
                  $(".modal[id*='popup_']").removeClass("is-active");
                  $("html").removeClass("is-clipped");
                }
              }
            });
          } else {
            alert(result.resultMsg);
          }
        });
      }
    }


