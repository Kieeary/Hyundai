    function heart(obj, blbsn){
        /*좋아요 카운트 증가,해제*/
  
        var $that = $(this);
        var $icoHeart = $(obj).find('.product-item-heart');
        var num = parseInt(removeComma($(obj).find('.product-item-heart__num').text()));
        $.ajax({
              url: "/community/mylove",
              data: {blbSn : blbsn},
              dataType:"json",
              success: function (result) {
                  if(result.resultData === "0000") {
                    if(!$icoHeart.hasClass('is-active')){
                      $icoHeart.addClass('is-active');
                      num = num + 1;
                    }else{
                      $icoHeart.removeClass('is-active');
                      num = num <= 0 ? 0 : num - 1;
                    }
                    var txtNum = num.toLocaleString();
                    $(obj).find('.product-item-heart__num').text(txtNum);
  
                  }else{
                      $.eshopfront.cmm.alert({
                          content : "좋아요는 한번만 가능합니다.",
                          btn: {
                                  close: false,
                                  cancel: false
                              }
